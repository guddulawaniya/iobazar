<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="HardcodedText,MissingConstraints"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="20dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/layout"
        android:padding="3dp"
        android:layout_marginTop="25dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/item_profile_img"
            android:layout_width="100dp"
            android:layout_height="100dp"

            android:layout_marginBottom="25dp"
            android:background="#dddddd"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription,MissingConstraints" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:layout_marginTop="20dp"

            android:orientation="vertical">
            <TextView
                android:id="@+id/item_student_name_title"
                android:layout_width="150dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_college"
                android:layout_width="200dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                app:layout_constraintTop_toBottomOf="@+id/item_student_name_title"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_specialization"
                android:layout_width="250dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                app:layout_constraintTop_toBottomOf="@+id/item_student_college"
                android:layout_marginLeft="15dp" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/item_student_description"
        android:layout_width="385dp"
        android:layout_height="20dp"
        android:layout_marginStart="3dp"
        android:layout_alignBottom="@id/layout"
        android:background="#dddddd"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="3dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/layout1"
        android:padding="3dp"
        android:layout_below="@+id/item_student_description"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/item_profile_img1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginBottom="25dp"
            android:background="#dddddd"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription,MissingConstraints" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:layout_marginTop="20dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/item_student_name_title1"
                android:layout_width="150dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_college1"
                android:layout_width="200dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                app:layout_constraintTop_toBottomOf="@+id/item_student_name_title"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_specialization1"
                android:layout_width="250dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                app:layout_constraintTop_toBottomOf="@+id/item_student_college"
                android:layout_marginLeft="15dp" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/item_student_description1"
        android:layout_width="385dp"
        android:layout_height="20dp"
        android:layout_marginStart="3dp"
        android:layout_alignBottom="@id/layout1"
        android:background="#dddddd"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="3dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/layout2"
        android:padding="3dp"
        android:layout_below="@+id/item_student_description1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/item_profile_img2"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginBottom="25dp"
            android:background="#dddddd"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription,MissingConstraints" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:layout_marginTop="20dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/item_student_name_title2"
                android:layout_width="150dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_college2"
                android:layout_width="200dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                app:layout_constraintTop_toBottomOf="@+id/item_student_name_title"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_specialization2"
                android:layout_width="250dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                app:layout_constraintTop_toBottomOf="@+id/item_student_college"
                android:layout_marginLeft="15dp" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/item_student_description2"
        android:layout_width="385dp"
        android:layout_height="20dp"
        android:layout_marginStart="3dp"
        android:layout_alignBottom="@id/layout2"
        android:background="#dddddd"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="3dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/layout3"
        android:padding="3dp"
