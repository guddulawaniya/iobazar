                app:layout_constraintTop_toBottomOf="@+id/item_student_college"
                android:layout_marginLeft="15dp" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/item_student_description3"
        android:layout_width="385dp"
        android:layout_height="20dp"
        android:layout_marginStart="3dp"
        android:layout_alignBottom="@id/layout3"
        android:background="#dddddd"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="3dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/layout4"
        android:padding="3dp"
        android:layout_below="@+id/item_student_description3"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/item_profile_img4"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginBottom="25dp"
            android:background="#dddddd"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription,MissingConstraints" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:layout_marginTop="20dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/item_student_name_title4"
                android:layout_width="150dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_college4"
                android:layout_width="200dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
                app:layout_constraintTop_toBottomOf="@+id/item_student_name_title"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/item_student_specialization4"
                android:layout_width="250dp"
                android:layout_height="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="8dp"
                android:background="#dddddd"
                app:layout_constraintStart_toEndOf="@+id/item_profile_img"
